@{
    Layout = null;
    Response.ContentType = "text/xml";   
}
@model IEnumerable<Mvc_ESM.Static_Helper.Event>
<data>
    @foreach (var myevent in Model) {
        <event id="@myevent.id">
            <start_date><![CDATA[@String.Format("{0:yyyy-MM-dd HH:mm:ss}",myevent.start_date)]]></start_date>
            <end_date><![CDATA[@String.Format("{0:yyyy-MM-dd HH:mm:ss}",myevent.end_date)]]></end_date>
            <text><![CDATA[@myevent.text]]></text>
            @*<details><![CDATA[@myevent.details]]></details>*@
        </event>
    } 
</data>